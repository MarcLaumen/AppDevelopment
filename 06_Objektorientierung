class Element
{
  String name;
  int preis;
  
  Element(String name,int preis)
  {
    this.name=name;
    this.preis=preis;
  }
  void zeigeWerteAn()
  {
    print(this.name);
    print(this.preis);     
  }
}
  
main()
{ 
  Element holz=Element("Holz",2);
    holz.zeigeWerteAn();
  Element stein=Element("Stein",5);
    stein.zeigeWerteAn();
  Element silber=Element("Silber",150);
    silber.zeigeWerteAn();
  Element gold=Element("Gold",200);
    gold.zeigeWerteAn();
}
